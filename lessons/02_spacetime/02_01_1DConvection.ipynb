   "outputs": [],
   "source": [
    "##problem parameters\n",
    "nx = 41\n",
    "dx = 2/(nx-1)\n",
    "nt = 10    \n",
    "dt = .02  \n",
    "x = numpy.linspace(0,2,nx)\n",
    "##initial conditions\n",
    "u = numpy.ones(nx)      #numpy function ones()\n",
    "lbound = numpy.where(x >= 0.5)[0]\n",
    "ubound = numpy.where(x <= 1)[0]\n",
    "u[numpy.intersect1d(lbound, ubound)]=2  \n",
    "\n"
   ]
